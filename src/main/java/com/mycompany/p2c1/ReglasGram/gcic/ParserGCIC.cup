	             							}
	             						}
             						:}
             |expr:e1 MENQ {:codigoHTML1 += " < ";:} 
             			expr:e2 	{:
             							if(e1 != null && e2 != null){
	             							String result = funOLM.menQ(e1, e2);
	             							if(result == null){
	             								addError("Semantico",e1,
	             								e1.getNombreToken()+" < "+ e2.getNombreToken() +" No esta definido",
	             								"<p>integer < integer</p> <p>decimal < decimal</p>");
